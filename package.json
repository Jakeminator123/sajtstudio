{
  "name": "sajtstudio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:safe": "node scripts/safe-dev-start.js",
    "dev:mobile": "node scripts/safe-dev-start-mobile.js",
    "dev:turbo": "next dev --turbopack",
    "kill": "node scripts/kill-node-processes.js",
    "kill:ps1": "pwsh -ExecutionPolicy Bypass -File scripts/kill-node-processes.ps1",
    "build": "next build",
    "start": "next start",
    "lint": "echo 'Linting is handled by Next.js during build. Run: npm run build'",
    "clean": "node scripts/clean.js",
    "clean:ps1": "pwsh -ExecutionPolicy Bypass -File scripts/quick-clean.ps1",
    "clean:full": "node scripts/clean-full.js",
    "rebuild": "npm run clean:full",
    "fix:warnings": "node scripts/auto-fix-warnings.js",
    "check:warnings": "node scripts/read-cursor-warnings.js",
    "check:errors": "node scripts/check-browser-errors.js"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "cheerio": "^1.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.0.0",
    "next": "16.0.1",
    "openai": "^6.9.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.4.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.0",
    "babel-plugin-react-compiler": "1.0.0",
    "critters": "^0.0.23",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4",
    "typescript": "^5"
  }
}
